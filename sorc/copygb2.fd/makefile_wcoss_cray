################################################################################
# This makefile was produced by fmjif at 17:21:11 on 97/02/24.
SHELL=	/bin/sh
CMD=	copygb2
FOPTS=  -g -O3 -r8 -convert big_endian -axCORE-AVX2
LOPTS=  -openmp
#INCS=	-I/nwprod/lib/incmod/g2_d
INCS= -I${G2_INCd}
OBJS=	copygb2.o
#LIBS= -L/nwprod/lib  -lg2_d -lw3nco_d -lbacio_4 -lip_d -lsp_d \
        -lpng -ljasper -lz
LIBS=   ${G2_LIBd} ${W3NCO_LIBd} ${BACIO_LIB4} ${IP_LIBd} ${SP_LIBd} ${PNG_LIB} ${JASPER_LIB} ${Z_LIB}
################################################################################
$(phony all):	$(CMD)
$(CMD):	$(OBJS)
	ftn $(LOPTS) $(OBJS) $(LIBS) -o $(CMD)
copygb2.o:	copygb2.f
	ftn -c $(INCS) $(FOPTS) $*.f
install:
	mkdir -p ../../exec
	cp -p $(CMD) ../../exec
clean:	
	-rm -f $(OBJS)
